{
  "name": "is-agentic-tui",
  "version": "0.0.1",
  "description": "Detect if code is running inside an agentic TUI (Claude Code, Cursor, Gemini CLI, etc.)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "claude",
    "claude-code",
    "cursor",
    "gemini",
    "aider",
    "agentic",
    "tui",
    "terminal",
    "detection"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
